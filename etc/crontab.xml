  -->
    <group id="default">
        <job name="messagequeue_clean_outdated_locks" instance="Magento\Framework\MessageQueue\Lock\WriterInterface" method="releaseOutdatedLocks">
            <schedule>0 * * * *</schedule>
        </job>
    </group>
    <group id="consumers">
        <job name="consumers_runner" instance="Magento\MessageQueue\Model\Cron\ConsumersRunner" method="run">
            <schedule>*/5 * * * *</schedule>
        </job>
    </group>
    <group id="default">
        <job name="mysqlmq_clean_messages" instance="Magento\MysqlMq\Model\Observer" method="cleanupMessages">
            <schedule>30 6,15 * * *</schedule>
        </job>
    </group>
    <group id="default">
        <job name="backend_expire_date" instance="Magento\NegotiableQuote\Cron\ExpireQuote" method="execute">
            <schedule>0 0 * * *</schedule>
        </job>
        <job name="negotiable_quote_send_emails" instance="Magento\NegotiableQuote\Cron\SendEmails" method="execute">
            <schedule>0 0 * * *</schedule>
        </job>
    </group>
    <group id="default">
        <job name="magento_newrelicreporting_cron" instance="Magento\NewRelicReporting\Model\Cron" method="runCron">
            <schedule>*/2 * * * *</schedule>
        </job>
    </group>
    <group id="default">
        <job name="newsletter_send_all" instance="Magento\Newsletter\Model\Observer" method="scheduledSend">
            <schedule>*/5 * * * *</schedule>
        </job>
    </group>
    <group id="default">
        <job name="paypal_fetch_settlement_reports" instance="Magento\Paypal\Cron\FetchReports" method="execute"/>
    </group>
    <group id="default">
        <job name="persistent_clear_expired" instance="Magento\Persistent\Observer\ClearExpiredCronJobObserver" method="execute">
            <schedule>0 0 * * *</schedule>
        </job>
    </group>
    <group id="default">
        <job name="catalog_product_alert" instance="Magento\ProductAlert\Model\Observer" method="process">
            <config_path>crontab/default/jobs/catalog_product_alert/schedule/cron_expr</config_path>
        </job>
    </group>
    <group id="default">
        <job name="send_notification" instance="Magento\Reminder\Cron\ScheduledNotification" method="execute">
            <schedule>*/15 * * * *</schedule>
        </job>
    </group>
    <group id="default">
        <job name="magento_reward_balance_warning_notification" instance="Magento\Reward\Cron\ScheduledBalanceExpireNotification" method="execute">
            <schedule>1 * * * *</schedule>
        </job>
        <job name="magento_reward_expire_points" instance="Magento\Reward\Cron\ScheduledPointsExpiration" method="execute">
            <schedule>1 * * * *</schedule>
        </job>
    </group>
    <group id="default">
        <job name="sales_clean_quotes" instance="Magento\Sales\Cron\CleanExpiredQuotes" method="execute">
            <schedule>0 0 * * *</schedule>
        </job>
        <job name="sales_clean_orders" instance="Magento\Sales\Model\CronJob\CleanExpiredOrders" method="execute">
            <schedule>0 * * * *</schedule>
        </job>
        <job name="aggregate_sales_report_order_data" instance="Magento\Sales\Model\CronJob\AggregateSalesReportOrderData" method="execute">
            <schedule>0 0 * * *</schedule>
        </job>
        <job name="aggregate_sales_report_invoiced_data"
             instance="Magento\Sales\Model\CronJob\AggregateSalesReportInvoicedData" method="execute">
            <schedule>0 0 * * *</schedule>
        </job>
        <job name="aggregate_sales_report_refunded_data" instance="Magento\Sales\Model\CronJob\AggregateSalesReportRefundedData" method="execute">
            <schedule>0 0 * * *</schedule>
        </job>
        <job name="aggregate_sales_report_bestsellers_data"
             instance="Magento\Sales\Model\CronJob\AggregateSalesReportBestsellersData" method="execute">
            <schedule>0 0 * * *</schedule>
        </job>
        <job name="sales_grid_order_async_insert" instance="SalesOrderIndexGridAsyncInsertCron" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="sales_grid_order_invoice_async_insert" instance="SalesInvoiceIndexGridAsyncInsertCron" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="sales_grid_order_shipment_async_insert" instance="SalesShipmentIndexGridAsyncInsertCron" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="sales_grid_order_creditmemo_async_insert" instance="SalesCreditmemoIndexGridAsyncInsertCron" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="sales_send_order_emails" instance="SalesOrderSendEmailsCron" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="sales_send_order_invoice_emails" instance="SalesInvoiceSendEmailsCron" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="sales_send_order_shipment_emails" instance="SalesShipmentSendEmailsCron" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="sales_send_order_creditmemo_emails" instance="SalesCreditmemoSendEmailsCron" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
    </group>
    <group id="default">
        <job name="magento_salesarchive_archive_orders" instance="Magento\SalesArchive\Cron\ArchiveOrdersByCron" method="execute">
            <schedule>0 0 * * *</schedule>
        </job>
    </group>
    <group id="default">
        <job name="aggregate_sales_report_coupons_data" instance="Magento\SalesRule\Cron\AggregateSalesReportCouponsData" method="execute">
            <schedule>0 0 * * *</schedule>
        </job>
    </group>
    <group id="default">
        <job name="magento_scheduled_import_export_log_clean" instance="Magento\ScheduledImportExport\Cron\ScheduledLogClean" method="execute" />
    </group>
    <group id="default">
        <job name="security_clean_admin_expired_sessions" instance="Magento\Security\Model\AdminSessionsManager" method="cleanExpiredSessions">
            <schedule>0 0 * * *</schedule>
        </job>
        <job name="security_clean_password_reset_request_event_records" instance="Magento\Security\Model\SecurityManager" method="cleanExpiredRecords">
            <schedule>0 0 * * *</schedule>
        </job>
    </group>
    <group id="default">
        <job name="aggregate_sales_report_shipment_data" instance="Magento\Shipping\Model\Observer" method="aggregateSalesReportShipmentData">
            <schedule>0 0 * * *</schedule>
        </job>
    </group>
    <group id="default">
        <job name="sitemap_generate" instance="Magento\Sitemap\Model\Observer" method="scheduledGenerateSitemaps" />
    </group>
    <group id="staging">
        <job name="staging_apply_version" instance="Magento\Staging\Model\StagingApplier" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="staging_remove_updates" instance="Magento\Staging\Model\Update\Cleaner" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
        <job name="staging_synchronize_entities_period" instance="Magento\Staging\Model\SynchronizeEntityPeriod" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
    </group>
    <group id="index">
        <job name="magento_targetrule_index_reindex" instance="Magento\TargetRule\Model\Indexer\TargetRule\Product\Rule" method="cleanByCron">
            <schedule>0 * * * *</schedule>
        </job>
    </group>
    <group id="default">
        <job name="aggregate_sales_report_tax_data" instance="Magento\Tax\Model\AggregateSalesReportTaxData"
             method="invoke">
            <schedule>0 0 * * *</schedule>
        </job>
      </group>
</config>
